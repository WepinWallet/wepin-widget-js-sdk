{
  "name": "@wepin/widget-sdk",
  "version": "0.4.1",
  "description": "Wepin Widget Javascript SDK for Web",
  "main": "./dist/wepin-widget-sdk.umd.js",
  "types": "./dist/src/index.d.ts",
  "author": "IoTrust, Co., Ltd.",
