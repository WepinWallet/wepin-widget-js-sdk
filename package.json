  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WepinWallet/wepin-widget-js-sdk.git"
  },
  "scripts": {
    "build-ts": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
